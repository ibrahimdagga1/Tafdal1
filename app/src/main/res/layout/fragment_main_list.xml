<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whitesmok"
    tools:context=".Fragment.MainList">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_216sdp"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="14dp"
        android:background="@color/white"
        android:layout_marginRight="14dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/main_list_image_food"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_width="@dimen/_108sdp"
            android:layout_height="@dimen/_135sdp"
            android:layout_gravity="center"
            android:src="@drawable/group_275" />

        <TextView
            android:id="@+id/main_list_tv_food"
            android:layout_marginTop="@dimen/_22sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_92sdp"
            android:layout_height="@dimen/_21sdp"
            android:gravity="center"
            android:textSize="@dimen/_16sdp"
            android:text="المواد الغذاية" />
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="@dimen/_220sdp"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:gravity="center"
        android:background=""
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:layout_weight="1"
            android:layout_marginEnd="@dimen/_11sdp"
            android:orientation="vertical"
            android:layout_marginRight="@dimen/_11sdp">

            <ImageView
                android:id="@+id/main_list_image_cleaning"
                android:layout_width="@dimen/_108sdp"
                android:layout_height="@dimen/_135sdp"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_20sdp"
                android:src="@drawable/repeat_grid_2" />

            <TextView
                android:id="@+id/main_list_tv_cleaning"
                android:layout_width="@dimen/_92sdp"
                android:layout_height="@dimen/_21sdp"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_22sdp"
                android:gravity="center"
                android:text="مواد التنظيف"
                android:textSize="@dimen/_16sdp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="wrap_content"
            android:background="@color/white"
            android:layout_height="match_parent"

            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/main_list_image_houseware"
                android:layout_width="@dimen/_108sdp"
                android:layout_height="@dimen/_135sdp"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_20sdp"
                android:src="@drawable/group_176" />

            <TextView
                android:id="@+id/main_list_tv_houseware"
                android:gravity="center"
                android:layout_width="@dimen/_92sdp"
                android:layout_height="@dimen/_21sdp"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_22sdp"
                android:text="الأدوات المنزلية"
                android:textSize="@dimen/_16sdp" />
        </LinearLayout>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>