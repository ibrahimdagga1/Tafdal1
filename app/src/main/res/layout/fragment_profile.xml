<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.ProfileFragment">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="414dp"
        android:layout_height="117dp"
        android:scaleType="centerCrop"
        android:src="@drawable/supermarketsavings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/register_et_name"
        android:layout_width="337dp"
        android:layout_height="45.58dp"
        android:layout_marginHorizontal="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:drawableRight="@drawable/icon_name"
        android:drawablePadding="10dp"
        android:ems="10"
        android:background="@drawable/shape_et_search"
        android:hint="ابراهيم عزمي"
        android:inputType="textPersonName"
        android:paddingHorizontal="18dp"
        android:paddingStart="18dp"
        android:paddingLeft="18dp"
        android:textColorHint="#939393"
        android:textSize="16dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <EditText
        android:id="@+id/register_et_mobile"
        android:layout_width="337dp"
        android:layout_height="45.58dp"
        android:layout_marginHorizontal="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:background="@drawable/shape_et_search"
        android:drawableRight="@drawable/icon_mobile"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="0599793707"
        android:inputType="textPersonName"
        android:paddingHorizontal="18dp"
        android:paddingStart="18dp"
        android:paddingLeft="18dp"
        android:textColorHint="#939393"
        android:textSize="16dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_et_name" />

    <EditText
        android:id="@+id/register_et_phone"
        android:layout_width="337dp"
        android:layout_height="45.58dp"
        android:layout_marginHorizontal="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:background="@drawable/shape_et_search"
        android:drawableRight="@drawable/phone_call__"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="082819751"
        android:inputType="textPersonName"
        android:paddingHorizontal="18dp"
        android:paddingStart="18dp"
        android:paddingLeft="18dp"
        android:textColorHint="#939393"
        android:textSize="16dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_et_mobile" />

    <EditText
        android:id="@+id/register_et_title"
        android:layout_width="337dp"
        android:layout_height="45.58dp"
        android:layout_marginHorizontal="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:background="@drawable/shape_et_search"
        android:drawableRight="@drawable/icon_map"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="غزة - الشجاعية - شارع بغداد"
        android:inputType="textPersonName"
        android:paddingHorizontal="18dp"
        android:paddingStart="18dp"
        android:paddingLeft="18dp"
        android:textColorHint="#939393"
        android:textSize="16dp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_et_phone" />



    <EditText
        android:id="@+id/register_et_email"
        android:layout_width="337dp"
        android:layout_height="45.58dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:ems="10"
        android:background="@drawable/shape_et_search"
        android:textSize="16dp"
        android:drawablePadding="10dp"
        android:textColorHint="#939393"
        android:textStyle="normal"
        android:drawableRight="@drawable/icon_email"
        android:inputType="textPersonName"
        android:paddingStart="18dp"
        android:layout_marginHorizontal="19dp"
        android:paddingHorizontal="18dp"
        android:hint="hisham@devany.com"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_et_title"

        android:paddingLeft="18dp" />

    <EditText
        android:id="@+id/register_et_password"
        android:layout_width="337dp"
        android:layout_height="45.58dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:ems="10"
        android:background="@drawable/shape_et_search"
        android:drawablePadding="10dp"
        android:textColorHint="#939393"
        android:textSize="16dp"

        android:inputType="textPersonName"
        android:layout_marginHorizontal="19dp"
        android:drawableRight="@drawable/icon_password"
        android:hint="*********"
        android:paddingStart="18dp"
        android:paddingHorizontal="@dimen/_18sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_et_email"
        android:paddingLeft="18dp"
     />

    <Button
        android:id="@+id/register_btn_login"
        android:layout_width="0dp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:layout_marginBottom="27dp"
        android:textSize="20sp"
        android:gravity="center"
        android:background="@drawable/shape_button"
        android:text="حفظ"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_et_password" />



</androidx.constraintlayout.widget.ConstraintLayout>